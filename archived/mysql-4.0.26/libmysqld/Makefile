# automake misses these
sql_yacc.cc sql_yacc.h: $(top_srcdir)/sql/sql_yacc.yy

#
# To make it easy for the end user to use the embedded library we
# generate a total libmysqld.a from all library files,

libmysqld.a:	libmysqld_int.a $(INC_LIB)
		if test "$(host_os)" = "netware" ; \
		then \
		  $(libmysqld_a_AR) libmysqld.a libmysqld_int.a $(INC_LIB) ; \
		else \
		  if test ! -d tmp ; then mkdir tmp ; fi ; \
		  rm -f $@ libmysqld_int2.a tmp/*.o tmp/*.a ; \
		  cp $(INC_LIB) tmp ; \
		  cp libmysqld_int.a libmysqld_int2.a ; \
		  cd tmp ; \
		  for file in *.a ; do \
		    bfile=`basename $$file .a` ; \
		    $(AR) x $$file; \
		    for obj in *.o ; do mv $$obj $${bfile}_$$obj ; done ; \
		    $(AR) q ../libmysqld_int2.a *.o ; \
		    rm -f *.o ; \
		  done ; \
		  cd .. ; \
		  mv libmysqld_int2.a libmysqld.a ; \
		  rm -f tmp/* ; \
		  $(RANLIB) libmysqld.a	; \
		fi

#libmysqld_la_LDFLAGS = -version-info 12:0:0
#CLEANFILES =		$(libmysqld_la_LIBADD) libmysqld.la

# This is called from the toplevel makefile
link_sources:
	  set -x; \
	  for f in $(sqlsources); do \
	    rm -f $(srcdir)/$$f; \
	    /bin/ln -s $(srcdir)/../sql/$$f $(srcdir)/$$f; \
	  done; \
	  for f in $(libmysqlsources); do \
	    rm -f $(srcdir)/$$f; \
	    /bin/ln -s $(srcdir)/../libmysql/$$f $(srcdir)/$$f; \
	  done

clean-local:
	rm -f `echo $(sqlsources) $(libmysqlsources) | sed "s;\.lo;.c;g"` \
	       $(top_srcdir)/linked_libmysqld_sources

# Don't update the files from bitkeeper
%::SCCS/s.%
# Tell versions [3.59,3.63) of GNU make to not export all variables.
# Otherwise a system limit (for SysV at least) may be exceeded.
.NOEXPORT:
