	$(CXXLINK) $(gen_lex_hash_LDFLAGS) $(gen_lex_hash_OBJECTS) $(gen_lex_hash_LDADD) $(LIBS)
mysqld$(EXEEXT): $(mysqld_OBJECTS) $(mysqld_DEPENDENCIES) 
	@rm -f mysqld$(EXEEXT)
	$(CXXLINK) $(mysqld_LDFLAGS) $(mysqld_OBJECTS) $(mysqld_LDADD) $(LIBS)

